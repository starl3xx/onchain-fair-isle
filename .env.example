# Contract Address (deployed on Base mainnet)
NEXT_PUBLIC_CONTRACT_ADDRESS=0x...

# Base Chain ID (8453 for mainnet, 84532 for sepolia)
NEXT_PUBLIC_CHAIN_ID=8453

# Your app's public URL
NEXT_PUBLIC_BASE_URL=https://your-domain.com

# Treasury wallet address (receives mint proceeds)
TREASURY_WALLET=0x...

# Base token URI (for contract deployment)
BASE_TOKEN_URI=https://your-domain.com/api/metadata/

# Max supply (0 for unlimited)
MAX_SUPPLY=0

# RPC URLs (optional, defaults to public endpoints)
BASE_RPC_URL=https://mainnet.base.org
BASE_SEPOLIA_RPC_URL=https://sepolia.base.org

# Private key for contract deployment (DO NOT COMMIT!)
PRIVATE_KEY=

# BaseScan API key for contract verification
BASESCAN_API_KEY=
